{
    "name": "indigophp/supervisor-configuration",
    "description": "Manage Supervisor configuration in PHP",
    "license": "MIT",
    "keywords": ["supervisor", "configuration"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/options-resolver": "~2.6.0"
    },
    "require-dev": {
        "league/flysystem": "~0.5.0",
        "phpspec/phpspec": "~2.1.0",
        "henrikbjorn/phpspec-code-coverage" : "~1.0.0"
    },
    "suggest": {
        "league/flysystem": "Used by Filesystem Parsers/Writers"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Supervisor\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Indigo\\Supervisor\\Stub\\": "stub/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
